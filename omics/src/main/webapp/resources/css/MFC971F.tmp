/* 메인 버튼 */
.mainbtn { width:980px; height:68px; border:1px  solid #ddd;}
.mainbtn li { float:left}
.mainbtn a { height:48px; display:inline-block; width:276px; padding:10px 25px;}
.mainbtn a:hover { height:48px; color:#fff; text-decoration:none; display:inline-block;width:276px; padding:10px 25px}
.mainbtn span {display:block; font-size:11px; padding-top:17px; letter-spacing:-0.01em}
.mainbtn strong { font-size:17px; font-weight:bold; color:#474e61; line-height:17px; margin-right:10px; }
.mainbtn a:hover strong { color:#fff;}
.mainbtn .manual { background:url(../images/main/manual1.gif) 53px 11px no-repeat #f9f9f9; border-right:solid 1px #ddd;}
.mainbtn .manual a:hover{  background:url(../images/main/manual2.gif) 53px 11px no-repeat #5a83d7;}
.mainbtn .manual span { padding-left:91px}
.mainbtn .down { background:url(../images/main/down1.gif) 25px 11px no-repeat #f9f9f9; border-right:solid 1px #ddd; }
.mainbtn .down a:hover{ background:url(../images/main/down2.gif) 25px 11px no-repeat #23b0c1; }
.mainbtn .down span { padding-left:63px}
.mainbtn .yt { background:url(../images/main/yt1.gif) 51px 11px no-repeat #f9f9f9;}
.mainbtn .yt a:hover{ background:url(../images/main/yt2.gif) 51px 11px no-repeat #1990eb;}
.mainbtn .yt span { padding-left:89px}


/* 링크 */
.link { background:url(../images/main/link_bg.gif) no-repeat; padding:10px 15px 12px 15px;}
.link li { display:inline; padding:0 13px;}


/* 하단 */
#footer { width:980px; margin:0 auto; border-top:2px #999 solid; height:65px; padding-top:21px;}
#footer .copy_logo { display:inline-block; float:left; padding-right:50px; margin-top:-7px;}
#footer address { font-size:12px; font-style:normal; color:#888; line-height:17px; margin-top:5px;}


/* 서브 */
.subcon {width:880px; margin:0 auto; padding:50px 0 80px 0; line-height:21px;}
.subcon h3 { clear:both; color:#444; letter-spacing:-0.05em; background:url(../images/sub/h3_bg.gif) bottom repeat-x; margin-bottom:40px; height:55px; padding-top:10px;}
.subcon h3 span { font-size:30px; line-height:30px;border-top:4px solid #5a83d7; padding-top:4px;}
/*.con a:hover { color:#1ca0de}*/
.subcon h4 { clear:both; font-size:15px; letter-spacing:-0.03em; background:url(../images/sub/h4.gif) left top no-repeat; padding-top:10px; margin-bottom:10px;}